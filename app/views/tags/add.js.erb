$("#tags_input_style").html("<%= j(render 'layouts/tags_input_style') %>")
$("#modal-tagging-list-of-<%= @user.id %>").html("<%= j(render 'taggings/modal_tagging_list', taggings: @taggings) %>")
$("#modal-tagging-form-of-<%= @user.id %>").html("<%= j(render 'taggings/modal_tagging_form', tag: @tag, user: @user ) %>")
$("#tagging-list-of-<%= @user.id %>").html("<%= j(render 'taggings/tagging_list', taggings: @taggings, user: @user) %>")
$("#common-users-of-<%= @user.id %>").html("<%= j(render partial: 'common_users') %>")
$("#tag-button-of-<%= @find_tag.id %>").html("<%= j(render partial: 'tag_button', locals: {tag: @find_tag} ) %>")
$("#tagging-users-count-of-<%= @find_tag.id %>").html("<%= j(render partial: 'tagging_users_count', locals: {tagging_users: @tagging_users }) %>")
$("#tagging-articles-count").html("<%= j(render partial: 'tagging_articles_count', locals: {tagging_articles: @tagging_articles}) %>")
$("#tag-mark-of-<%= @find_tag.id %>").html("<%= j(render partial: 'tag_mark', locals: {tag: @find_tag} ) %>")
$("#tags_input_script").html("<%= j(render 'layouts/tags_input_script') %>")